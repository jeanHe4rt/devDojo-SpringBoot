-- Database: SpringBoot

-- DROP DATABASE "SpringBoot";

CREATE DATABASE "SpringBoot"
    WITH 
    OWNER = jean
    ENCODING = 'UTF8'
    LC_COLLATE = 'pt_BR.UTF-8'
    LC_CTYPE = 'pt_BR.UTF-8'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;

CREATE TABLE Student (
	studentId SERIAL PRIMARY KEY NOT NULL,
	name VARCHAR(100) NOT NULL
);